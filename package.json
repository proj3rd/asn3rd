  },
  "homepage": "https://github.com/proj3rd/asn3rd#readme",
  "devDependencies": {
    "@types/antlr4": "^4.7.2",
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "antlr4": "^4.9.3"
  }
}
